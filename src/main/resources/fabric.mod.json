{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name} [C]",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${url_homepage}",
    "sources": "${git_sources}",
    "issues": "${git_issues}"
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "${mod_environment}",
  "entrypoints": {
    "main": [
      "maven_group.maven_entrypoint"
    ],
    "modmenu": [
      "maven_group.config.ModMenuIntegration"
    ]
  },
  "accessWidener": "${mod_id}.accesswidener",
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "${minecraft_version}",
    "cloth-config": "*"
  },
  "recommends": {
    "modmenu": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.${url_type}": "${url_homepage}"
      }
    }
  }
}
